{
    "manifest_version": 3,
    "name": "startpage",
    "short_name": "startpage",
    "version": "1.1",
    "description": "Overrides the browsers newtab page with a custom minimal startpage.",
    "author": "zionsenpai",
    "icons": {
      "64": "img/icon64.png"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": [
      "storage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "startpage@zionsenpai.github.io/Personal-Portfolio/"
      }
    }
  }